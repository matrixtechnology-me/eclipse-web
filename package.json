{
  "name": "eclipse",
  "version": "0.1.0-rc",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "postinstall": "npx prisma generate",
    "start": "next start",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "vitest",
    "test:e2e": "cypress open",
    "prisma:push": "prisma db push",
    "prisma:create-migration": "prisma migrate dev --create-only",
    "prisma:apply-migration": "prisma migrate dev",
    "prisma:reset": "prisma migrate reset",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "prisma:validate": "prisma validate",
    "prisma:sort": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/utils/sort.ts ",
    "prisma:seed": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/utils/seed.ts ",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "tailwind:build": "tailwindcss -o styles/tailwind.css --minify",
    "shadcn:add": "pnpm dlx shadcn@latest add",
    "shadcn:update": "pnpm dlx shadcn@latest update",
    "shadcn:remove": "pnpm dlx shadcn@latest remove",
    "services:up": "docker compose -f infra/docker-compose.yaml up -d",
    "services:stop": "docker compose -f infra/docker-compose.yaml stop",
    "services:down": "docker compose -f infra/docker-compose.yaml down",
    "docker:build": "docker build -f infra/Dockerfile ."
  },
  "dependencies": {
